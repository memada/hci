{
  "manifest_version": 2,
  "name": "HCI",
  "description": "HCI project",
  "version": "1.0",
  "options_page": "app/settings.html",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
     "matches": ["<all_urls>"],
      "js": ["app/ui/jquery/jquery.min.js", "app/ui/jquery/jquery-ui.js", "app/ui/manipulation.js", "app/ui/constants.js", "app/ui/custom.js"]
    }
  ]
}